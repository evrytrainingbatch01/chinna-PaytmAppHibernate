<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.evry.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.evry.controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AccountServlet</servlet-name>
  	<display-name>AccountServlet</display-name>
  	<description></description>
  	<servlet-class>com.evry.controller.AccountServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TransferAccountServlet</servlet-name>
  	<display-name>TransferAccountServlet</display-name>
  	<description></description>
  	<servlet-class>com.evry.controller.TransferAccountServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AccountServlet</servlet-name>
  	<url-pattern>/AccountServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TransferAccountServlet</servlet-name>
  	<url-pattern>/TransferAccountServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
